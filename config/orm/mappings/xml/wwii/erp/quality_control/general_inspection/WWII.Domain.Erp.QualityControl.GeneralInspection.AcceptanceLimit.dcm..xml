<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity
        name="WWII\Domain\Erp\QualityControl\GeneralInspection\AcceptanceLimnit"
        table="qc_general_inspection_acceptance_index">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <many-to-one
            field="category"
            target-entity="WWII\Domain\Erp\QualityControl\GeneralInspection\Category" />

        <field
            name="sampleSize"
            column="sampleSize"
            type="integer" />

        <many-to-one
            field="acceptanceIndex"
            target-entity="WWII\Domain\Erp\QualityControl\GeneralInspection\AcceptanceIndex" />

        <field
            name="limit"
            column="limit"
            type="integer" />

    </entity>
</doctrine-mapping>
