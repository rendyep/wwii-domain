<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="WWII\Domain\Erp\QualityControl\GeneralInspection\LootRange" table="qc_general_inspection_loot_range">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <field
            name="minLoot"
            column="min_loot"
            type="integer" />

        <field
            name="maxLoot"
            column="max_loot"
            type="integer"
            nullable="true" />

        <many-to-one
            field="level"
            target-entity="WWII\Domain\Erp\QualityControl\GeneralInspection\Level" />

        <many-to-one
            field="category"
            target-entity="WWII\Domain\Erp\QualityControl\GeneralInspection\Category" />

    </entity>
</doctrine-mapping>
