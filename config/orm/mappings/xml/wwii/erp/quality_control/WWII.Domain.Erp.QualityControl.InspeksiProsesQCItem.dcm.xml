<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="WWII\Domain\Erp\QualityControl\InspeksiProsesQCItem" table="erp_qc_inspeksi_proses_qc_item">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <field
            name="waktu"
            column="waktu"
            type="time" />

        <field
            name="kodeProduk"
            column="kode_produk"
            type="string"
            length="50" />

        <field
            name="namaProduk"
            column="namaProduk"
            type="string"
            length="50" />

        <field
            name="jumlahItemTerinspeksi"
            column="jumlah_item_terinspeksi"
            type="integer" />

        <field
            name="jumlahItemTergores"
            column="jumlah_item_tergores"
            type="integer" />

        <field
            name="jumlahItemTerpolusi"
            column="jumlah_item_terpolusi"
            type="integer" />

        <field
            name="jumlahItemSalahUkuran"
            column="jumlah_item_salah_ukuran"
            type="integer" />

        <field
            name="jumlahItemKelebihanLem"
            column="jumlah_item_kelebihan_lem"
            type="integer" />

        <field
            name="jumlahItemKelebihanCat"
            column="jumlah_item_kelebihan_cat"
            type="integer" />

        <field
            name="jumlahItemSalahWarna"
            column="jumlah_item_salah_warna"
            type="integer" />

        <field
            name="jumlahItemBergelembung"
            column="jumlah_item_bergelembung"
            type="integer" />

        <field
            name="jumlahItemStrukturLonggar"
            column="jumlah_item_struktur_longgar"
            type="integer" />

        <field
            name="jumlahItemCoverTerpotong"
            column="jumlah_item_cover_terpotong"
            type="integer" />

        <field
            name="jumlahItemArahHorizontal"
            column="jumlah_item_arah_horizontal"
            type="integer" />

        <field
            name="jumlahItemSandingBuruk"
            column="jumlah_item_sanding_buruk"
            type="integer" />

        <field
            name="jumlahItemPakuKeluar"
            column="jumlah_item_paku_keluar"
            type="integer" />

        <field
            name="jumlahItemLemDegumming"
            column="jumlah_item_lem_degumming"
            type="integer" />

        <field
            name="jumlahItemGap"
            column="jumlah_item_gap"
            type="integer" />

        <field
            name="jumlahItemCacatLainnya"
            column="jumlah_item_cacat_lainnya"
            type="integer" />

        <field
            name="jumlahItemKurang"
            column="jumlah_item_kurang"
            type="integer" />

        <many-to-one
            field="inspeksiProsesQC"
            target-entity="WWII\Domain\Erp\QualityControl\InspeksiProsesQC"
            inversed-by="items" />

    </entity>
</doctrine-mapping>
