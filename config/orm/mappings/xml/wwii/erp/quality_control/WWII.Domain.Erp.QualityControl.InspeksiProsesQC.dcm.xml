<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="WWII\Domain\Erp\QualityControl\InspeksiProsesQC" table="erp_qc_inspeksi_proses_qc">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <field
            name="tanggal"
            column="tanggal"
            type="date" />

        <field
            name="group"
            column="group"
            type="string"
            length="50" />

        <field
            name="lokasi"
            column="lokasi"
            type="string"
            length="50" />

        <field
            name="pelaksana"
            column="pelaksana"
            type="string"
            length="50" />

        <one-to-many
            field="items"
            target-entity="WWII\Domain\Erp\QualityControl\InspeksiProsesQCItem"
            mapped-by="inspeksiProsesQC">
            <cascade>
                <cascade-all />
            </cascade>
        </one-to-many>

    </entity>
</doctrine-mapping>
